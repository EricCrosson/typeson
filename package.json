{
  "name": "typeson",
  "version": "2.1.4",
  "description": "Preserves types over JSON, BSON or socket.io",
  "main": "typeson.js",
  "scripts": {
    "test": "node test.js",
    "build": "node_modules/.bin/uglifyjs typeson.js -o typeson.min.js --source-map typeson.min.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfahlander/typeson.git"
  },
  "keywords": [
    "JSON",
    "remoting",
    "serialization",
    "types"
  ],
  "author": "dfahlander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dfahlander/typeson/issues"
  },
  "homepage": "https://github.com/dfahlander/typeson#readme",
  "devDependencies": {
    "uglify-js": "^2.6.2"
  },
  "tonicExample": "var Typeson = require('typeson');\nvar TSON = new Typeson()\n        .register(require('typeson-registry/presets/builtin'));\n\nTSON.stringify({foo: /bar/gi});"
}
